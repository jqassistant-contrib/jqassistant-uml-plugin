<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.8" name="XMI"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.8 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.8.xsd">
    <description>The jQAssistant plugin for importing XMI models. It provides a scanner for reading XMI files and
        rules for extracting UML models which can be used to verify architectural concepts and constraints in a project.
    </description>
    <model>
        <class>org.jqassistant.contrib.plugin.xmi.api.XMIFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.XMIDocumentationDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.XMIAppliedStereotypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.XMIStereotypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.XMITaggedValueDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLModelDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLOwnedAttributeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLOwnedEndDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLProvidedInterfaceDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLRequiredInterfaceDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLPackagedElementDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLProfileApplicationDescriptor</class>
        <class>org.jqassistant.contrib.plugin.xmi.api.UMLAppliedProfileDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.xmi.impl.scanner.XMIFileScannerPlugin</class>
    </scanner>
    <rules>
        <resource>xmi.xml</resource>
    </rules>
</jqassistant-plugin>
